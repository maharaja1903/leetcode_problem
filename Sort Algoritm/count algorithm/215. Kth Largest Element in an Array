class Solution {
    public int findKthLargest(int[] nums, int k) {
        Arrays.sort(nums);
        return nums[nums.length-k];
    }
}
//count algorithm
class Solution {
    public int findKthLargest(int[] nums, int k) {
        int[] place=new int[20001];
        for(int i=0;i<nums.length;i++){
            place[nums[i]+10000]++;
        }
        for(int i=place.length-1;i>=0;i--){
            if(place[i]>0){
                k=k-place[i];
                if(k<=0)return i-10000;
            }
        }
        return -1;
    }
}


