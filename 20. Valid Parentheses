class Solution {
    public boolean isValid(String s) {
        if (s.length() % 2 != 0) return false;
        
        Stack<Character> res =new Stack<>();
        for(int i=0;i<s.length();i++){
            char c=s.charAt(i);
            if(c =='[' || c =='{' || c =='(')res.push(c);
            else if(c == ']'){
                if(!res.isEmpty() && res.peek()=='[' ){
                    res.pop();
                }else return false;
            }
            else if(c == ')'){
                if(!res.isEmpty() && res.peek()=='('){
                    res.pop();
                }else return false;
            }
            else if(c == '}'){
                if(!res.isEmpty() && res.peek()=='{'){
                    res.pop();
                }else return false;
            }
        }
        
        return res.isEmpty();
    }
}
